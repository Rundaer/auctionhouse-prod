{% extends 'base.html.twig' %}

{% block title %}Moje Aukcje!{% endblock %}

{% block body %}
    
    {% for auction in auctions %}
        <h2><a href={{ url('my_auction_details', {"id": auction.id} )}}>{{ auction.title }}</a></h2>
        <p>opis: {{ auction.description }}</p>
        <p>cena: {{ auction.price }}</p>
        <p>cena wywo≈Çawca {{ auction.startingPrice }}</p>
        <p>Koniec: {{ auction.expiresAt | date("Y-m-d H:i:s") }}</p>
        <hr>
    {% endfor %}

{% endblock %}
